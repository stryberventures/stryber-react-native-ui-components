{
  "name": "@stryberventures/stryber-react-native-ui-components",
  "version": "0.0.16",
  "main": "lib/commonjs/index.js",
  "react-native": "lib/module/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/components/index.d.ts",
  "homepage": "https://matterhorn.stryber.com/native/components",
  "keywords": [
    "react-native",
    "reactjs",
    "reactnative",
    "bootstrap"
  ],
  "description": "UI library for React Native",
  "repository": {
    "type": "git",
    "url": "https://github.com/stryberventures/stryber-react-native-ui-components"
  },
  "scripts": {
    "start": "npx react-native start",
    "ios": "npx react-native run-ios",
    "android": "npx react-native run-android",
    "test": "jest",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write src/**/*.ts*",
    "storybook": "start-storybook -p 7007 -c ./src/storybook",
    "prepare": "bob build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js)": [
      "yarn lint:write",
      "git add"
    ]
  },
  "peerDependencies": {
    "@react-native-community/async-storage": ">=1.6.3",
    "@react-native-community/datetimepicker": ">=2.1.0",
    "react": ">=16.8.6",
    "react-native": ">=0.60.5",
    "react-native-gesture-handler": ">=1.5.2",
    "react-native-linear-gradient": ">=2.5.6",
    "react-native-reanimated": ">=1.3.0",
    "react-native-svg": ">=9.13.2",
    "rn-fetch-blob": ">=0.11.2"
  },
  "dependencies": {
    "jest-static-stubs": "0.0.1",
    "lodash-es": "^4.17.15",
    "shorthash": "^0.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime": "^7.6.0",
    "@react-native-community/async-storage": "^1.6.3",
    "@react-native-community/bob": "^0.10.0",
    "@react-native-community/datetimepicker": "^2.1.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-notes": "^5.2.8",
    "@storybook/addon-ondevice-actions": "^5.2.8",
    "@storybook/addon-ondevice-backgrounds": "^5.2.8",
    "@storybook/addon-ondevice-knobs": "^5.2.8",
    "@storybook/addon-ondevice-notes": "^5.2.8",
    "@storybook/addon-options": "^5.2.8",
    "@storybook/addon-storyshots": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react-native": "^5.2.8",
    "@storybook/react-native-server": "^5.2.8",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.22",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.7.2",
    "formik": "^2.0.6",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "moment": "^2.24.0",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^1.3.0",
    "react-native-svg": "^9.13.2",
    "react-native-vector-icons": "^6.6.0",
    "react-test-renderer": "16.8.6",
    "rn-fetch-blob": "^0.11.2",
    "typescript": "^3.8.3",
    "yup": "^0.27.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg)$": "RelativeImageStub",
      ".+\\.(png)$": "jest-static-stubs/png"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(add.js)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "@react-native-community/bob": {
    "source": "src/components",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
