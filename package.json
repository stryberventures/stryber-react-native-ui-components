{
  "name": "@stryberventures/stryber-react-native-ui-components",
  "version": "0.0.67",
  "main": "lib/commonjs/index.js",
  "react-native": "lib/module/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "homepage": "https://matterhorn.stryber.com/native/components",
  "keywords": [
    "react-native",
    "reactjs",
    "reactnative",
    "bootstrap"
  ],
  "description": "UI library for React Native",
  "repository": {
    "type": "git",
    "url": "https://github.com/stryberventures/stryber-react-native-ui-components"
  },
  "scripts": {
    "start": "npx react-native start",
    "ios": "npx react-native run-ios",
    "android": "npx react-native run-android",
    "test": "jest",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write src/**/*.ts*",
    "storybook": "start-storybook -p 7007",
    "prepare": "bob build",
    "publish": "npm publish --access public",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.story.tsx",
      "outputFile": "./src/storyLoader.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js)": [
      "yarn lint:write",
      "git add"
    ]
  },
  "peerDependencies": {
    "@react-native-community/async-storage": ">=1.6.3",
    "react": ">=16.8.6",
    "react-native": ">=0.60.5",
    "react-native-date-picker": "^3.0.0",
    "react-native-gesture-handler": ">=1.5.2",
    "react-native-reanimated": ">=1.3.0",
    "react-native-svg": ">=9.13.2",
    "rn-fetch-blob": ">=0.11.2"
  },
  "dependencies": {
    "jest-static-stubs": "0.0.1",
    "lodash-es": "^4.17.15",
    "shorthash": "^0.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/async-storage": "^1.6.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-backgrounds": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-ondevice-notes": "^5.3.23",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storyshots": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "eslint": "^7.14.0",
    "formik": "^2.1.4",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.2",
    "metro-react-native-babel-preset": "^0.66.0",
    "moment": "^2.25.3",
    "react": "17.0.2",
    "react-native": "0.65.0",
    "react-native-builder-bob": "^0.18.1",
    "react-native-codegen": "^0.0.7",
    "react-native-date-picker": "^3.0.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^1.3.0",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-svg": "^9.13.2",
    "react-test-renderer": "17.0.1",
    "rn-fetch-blob": "^0.11.2",
    "typescript": "^3.8.3",
    "yup": "^0.28.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg)$": "RelativeImageStub",
      ".+\\.(png)$": "jest-static-stubs/png"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(add.js)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "react-native-builder-bob": {
    "source": "src/components",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/react-native-typescript@0.0.5": {
          "rawConfig": {
            "compilerPath": "typescript/bin/tsc",
            "compilerArguments": [
              "--declaration"
            ],
            "compiledFileTypes": [
              ".ts",
              ".tsx"
            ],
            "configFileName": "tsconfig.json",
            "tsconfig": {
              "compilerOptions": {
                "rootDir": "./",
                "outDir": "dist",
                "target": "esnext",
                "module": "commonjs",
                "moduleResolution": "node",
                "lib": [
                  "es6"
                ],
                "allowJs": true,
                "jsx": "react",
                "esModuleInterop": true,
                "resolveJsonModule": true,
                "experimentalDecorators": true,
                "sourceMap": true,
                "declaration": true,
                "isolatedModules": false,
                "allowSyntheticDefaultImports": true,
                "inlineSourceMap": false,
                "inlineSources": true,
                "removeComments": false,
                "typeRoots": [
                  "./node_modules/@types"
                ],
                "importHelpers": false
              },
              "include": [
                "./**/*"
              ],
              "exclude": [
                "node_modules",
                ".dependencies",
                "dist"
              ]
            },
            "development": true,
            "copyPolicy": {
              "ignorePatterns": [
                "package.json",
                "package-lock.json",
                "tsconfig.json"
              ],
              "disable": false
            }
          }
        }
      }
    },
    "componentsDefaultDirectory": "src/components/{name}",
    "packageManager": "yarn",
    "defaultScope": "stryber.matterhorn-native"
  }
}
